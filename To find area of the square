import java.util.Scanner;
public class Areaofsquare {
  
	public static void main(String[] args) {
		
		System.out.println("----------------TO FIND AREA OF THE SQUARE---------------------");
		
		Scanner side1 = new Scanner(System.in);
		System.out.print("Enter the side one of the square:");
		
		double sidea = side1.nextDouble();
		
		Scanner side2 = new Scanner(System.in);
		System.out.print("Enter the side two of the square:");
		
		double sideb = side2.nextDouble();
		
		double area = sidea * sideb;
		
		System.out.println("The area of the square is:"+ area);
		
		
		System.out.println("Done by irshadhullah");
	}
}
